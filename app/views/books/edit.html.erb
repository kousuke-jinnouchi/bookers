<h2>Editing book</h2>
<%= form_with model: @book do |f| %>
  <div>Title</div>
  <%= f.text_field :title %>
  
  <div>Body</div>
  <%= f.text_area :body %>

  <div>
    <%= f.submit 'Update Book' %>
  </div>
  <%= link_to "Show", book_path(@book.id) %>
  　|　
  <%= link_to "Back", "/books" %>
<% end %>
